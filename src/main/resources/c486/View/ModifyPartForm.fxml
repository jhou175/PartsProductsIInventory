<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="ModifyPartAnchorPane" prefHeight="700.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ModifyPartController">
   <children>
      <Label fx:id="modifyPartLbl" layoutX="50.0" layoutY="77.0" text="Modify Part">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <RadioButton fx:id="inHouseRadio" layoutX="190.0" layoutY="80.0" mnemonicParsing="false" onAction="#onInHouse" selected="true" text="In-House">
         <font>
            <Font size="14.0" />
         </font>
         <toggleGroup>
            <ToggleGroup fx:id="tGroup" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="outsourcedRadio" layoutX="380.0" layoutY="80.0" mnemonicParsing="false" onAction="#onOutsourced" text="Outsourced" toggleGroup="$tGroup">
         <font>
            <Font size="14.0" />
         </font>
      </RadioButton>
      <Label fx:id="IDLbl" layoutX="80.0" layoutY="170.0" prefHeight="20.0" prefWidth="14.0" text="ID">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="IDTxt" editable="false" layoutX="190.0" layoutY="170.0" />
      <Label fx:id="nameLbl" layoutX="80.0" layoutY="220.0" text="Name">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="nameTxt" layoutX="190.0" layoutY="220.0" />
      <Label fx:id="invLbl" layoutX="80.0" layoutY="260.0" text="Inv">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="inventoryTxt" layoutX="190.0" layoutY="260.0" />
      <Label fx:id="priceCostLbl" layoutX="80.0" layoutY="300.0" text="Price/Cost">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="priceCostTxt" layoutX="190.0" layoutY="300.0" />
      <Label fx:id="maxLbl" layoutX="80.0" layoutY="340.0" text="Max">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="maxTxt" layoutX="190.0" layoutY="340.0" />
      <Label fx:id="selectedOptionLbl" layoutX="80.0" layoutY="380.0" text="Machine ID">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="selectedOptionTxt" layoutX="190.0" layoutY="380.0" />
      <Label fx:id="minLbl" layoutX="380.0" layoutY="340.0" text="Min">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="minTxt" layoutX="442.0" layoutY="340.0" />
      <Button fx:id="cancelButton" layoutX="462.0" layoutY="463.0" mnemonicParsing="false" onAction="#cancelToMain" text="Cancel">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="saveButton" layoutX="380.0" layoutY="463.0" mnemonicParsing="false" onAction="#saveModifyPart" text="Save">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
